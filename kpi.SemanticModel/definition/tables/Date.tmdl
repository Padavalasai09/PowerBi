	partition Date = calculated
		mode: import
		source =
				
				VAR MinDateKey = MIN(Sales[DateKey])
				VAR MaxDateKey = MAX(Sales[DateKey])
				// Convert DateKey (YYYYMMDD) to date format
				VAR MinDate = DATE(INT(MinDateKey/10000), INT(MOD(MinDateKey,10000)/100), MOD(MinDateKey,100))
				VAR MaxDate = DATE(INT(MaxDateKey/10000), INT(MOD(MaxDateKey,10000)/100), MOD(MaxDateKey,100))
				RETURN
				ADDCOLUMNS(
				    CALENDAR(MinDate, MaxDate),
				    // Recreate DateKey for relationship
				    "DateKey", YEAR([Date]) * 10000 + MONTH([Date]) * 100 + DAY([Date]),
				    // Hierarchical columns
				    "Year", YEAR([Date]),
				    "Quarter", "Q" & QUARTER([Date]),
				    "MonthNo", MONTH([Date]),
				    "Month", FORMAT([Date], "MMMM"),
				    "YearQuarter", FORMAT([Date], "YYYY") & " Q" & QUARTER([Date]),
				    "YearMonth", FORMAT([Date], "YYYY-MM"),
				    "DayOfWeek", FORMAT([Date], "dddd"),
				    "DayOfWeekShort", FORMAT([Date], "ddd")
				)

	annotation PBI_Id = 4083623652bb4dfb996254f49cc093f6

